  "dependencies": {
    "@prisma/client": "^4.11.0",
    "@types/helmet": "^4.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "prisma": "^4.11.0",
    "shapely": "^0.2.3"
